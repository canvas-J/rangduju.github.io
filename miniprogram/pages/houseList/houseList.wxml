<!--miniprogram/pages/houseList/houseList.wxml-->
<view class="weui-cells__title" style="text-align:center"> 已托管房源{{houselist.length}}套 </view>

<view wx:for="{{houselist}}" wx:for-item="i">
  <view class="weui-cells weui-cells_after-title">
    <view class="weui-cell weui-cell_access" bindtap="toHousePage" id="{{i}}">
      <view class="weui-cell__bd">{{i}}</view>
      <view class="weui-cell__ft weui-cell__ft_in-access" style="font-size: 0">
        <view  style="display: inline-block;vertical-align:middle; font-size: 17px;">查看详情</view>
      </view>
    </view>
  </view>
</view>